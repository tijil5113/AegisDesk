{
  "version": 2,
  "builds": [
    {
      "src": "api/chat.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/mail.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/mail/(.*)",
      "dest": "/api/mail.js"
    }
  ],
  "env": {
    "API_BASE_URL": "@api_base_url",
    "ALLOWED_ORIGIN": "@allowed_origin",
    "GMAIL_CLIENT_ID": "@gmail_client_id",
    "OUTLOOK_CLIENT_ID": "@outlook_client_id"
  }
}
